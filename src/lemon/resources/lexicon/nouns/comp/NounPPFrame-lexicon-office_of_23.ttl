  lemon:reference           <http://dbpedia.org/ontology/office> ;
  lemon:subjOfProp          :arg2-147-147-147-148-148-148-148-148-148-148-145-145-145-145-143-143-143-143-64-64-633_office_of_22 ;
  lemon:objOfProp           :arg1-147-147-147-148-148-148-148-148-148-148-145-145-145-145-143-143-143-143-64-64-633_office_of_22 ;
  lemon:condition           :office_of_22_sense_1_condition .

:office_of_22_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2-147-147-147-148-148-148-148-148-148-148-145-145-145-145-143-143-143-143-64-64-633_office_of_22 lemon:marker :office_of_22_form_preposition .
